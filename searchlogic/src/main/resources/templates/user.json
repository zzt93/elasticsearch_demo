{
  "template": "user",
  "mappings" : {
    "user" : {
      "properties" : {
        "superId" : {
          "type" : "keyword"
        },
        "tags" : {
          "type" : "nested",
          "properties" : {
            "des" : {
              "type" : "keyword"
            }
          }
        },
        "username" : {
          "type": "text",
          "analyzer": "smartcn"
        } ,
        "email" : {
          "type" : "text",
          "analyzer" : "smartcn"
        },
        "mobile" : {
          "type" : "text",
          "analyzer" : "smartcn"
        }
      }
    }
  },
  "settings" : {
    "index" : {
      "refresh_interval" : "1s",
      "number_of_shards" : "10",
      "store" : {
        "type" : "fs"
      },
      "number_of_replicas" : "1"
    }
  }
}